{
  "icons": {
    "16": "./gen-assets/icon16.plasmo.png",
    "32": "./gen-assets/icon32.plasmo.png",
    "48": "./gen-assets/icon48.plasmo.png",
    "64": "./gen-assets/icon64.plasmo.png",
    "128": "./gen-assets/icon128.plasmo.png"
  },
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.plasmo.png",
      "32": "./gen-assets/icon32.plasmo.png",
      "48": "./gen-assets/icon48.plasmo.png",
      "64": "./gen-assets/icon64.plasmo.png",
      "128": "./gen-assets/icon128.plasmo.png"
    },
    "default_popup": "./popup.html"
  },
  "version": "0.0.1",
  "author": "Ajikaba",
  "name": "DEV | Workout nightmare",
  "description": "This a extension which forces you to do intense workout and give nightmares possibly :)",
  "background": {
    "service_worker": "./static/background/index.ts"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.plasmo.com/*"
      ],
      "js": [
        "../src/contents/plasmo.ts"
      ]
    }
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
